/*Вводится один символ, измените его регистр. 
То есть, если была введена строчная буква - сделайте ее заглавной и наоборот. 
Символы, не являющиеся латинской буквой, нужно выводить без изменений. */

#include <iostream>
#include<string>
using namespace std;

int main() {
	string a;
	char c;
	cin >> a;

	string b="";
	for (auto c : a) {
		//перевод малньких букв в большие
		if (c >= 'a'&& c <= 'z') {
			int al_num = c - 'a';
			b += 'A' +al_num;
		}
		//перевод больших в маленькие
		else if (c>='A' && c<='Z') {
			int al_num = c - 'A';   //ищем номер буквы в алфавите
			b += 'a' + al_num;
		}
		//иначе выводим символ без изменений
		else {
			cout << a;
		}

	}
	cout << b;
			
	return 0;
}
